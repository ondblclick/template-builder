<% if (items.find(i => i.folder)) { %>
  <%- include('/partials/mixedtype/block.ejs', { items: items.filter(i => i.folder) }) %>
<% } %>

<% if (items.find(i => i.image)) { %>
  <tr>
    <td colspan="4">
      <p class="p-heading">Photos & Videos</p>
    </td>
  </tr>
  <%- include('/partials/mixedtype/block.ejs', { items: items.filter(i => i.image) }) %>
<% } %>

<% if (items.find(i => i.other)) { %>
  <tr>
    <td colspan="4">
      <p class="p-heading">Files</p>
    </td>
  </tr>
  <%- include('/partials/mixedtype/block.ejs', { items: items.filter(i => i.other) }) %>
<% } %>
